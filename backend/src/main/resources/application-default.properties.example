# Copy this file to application-default.properties and fill in your values
# DO NOT commit application-default.properties to version control!

GOOGLE_CLIENT_ID = your-google-client-id.apps.googleusercontent.com

DB_URL = jdbc:postgresql://localhost:5432/your_database
DB_USERNAME = your_db_user
DB_PASSWORD = your_db_password

B2_ENDPOINT = https://s3.your-region.backblazeb2.com
B2_REGION = your-region
B2_BUCKET = your-bucket-name
B2_ACCESS_KEY = your_b2_access_key
B2_SECRET_KEY = your_b2_secret_key

# Generate a secure secret: openssl rand -base64 64
JWT_SECRET = your_base64_encoded_jwt_secret_at_least_256_bits
JWT_EXPIRATION_MS = 86400000

CORS_ALLOWED_ORIGINS = http://localhost:3000,http://localhost:5173
